{
	"name": "blog",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/blog/src",
	"projectType": "app",
	"targets": {
		"dev": {
			"executor": "nx:run-commands",
			"options": {
				"commands": ["npm run dev:tailwind", "npm run serve"],
				"cwd": "apps/blog",
				"parallel": true
			}
		},
		"deploy": {
			"dependsOn": ["build"],
			"executor": "nx:run-script",
			"options": {
				"script": "deploy"
			}
		}
	},
	"tags": []
}
